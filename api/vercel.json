{
  "version": 2,
  "builds": [
    {
      "src": "pages/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/key-generation", "dest": "/pages/key-generation.html" },
    { "src": "/api/generate-key", "dest": "/api/generate-key.js" },
    { "src": "/api/validate-key", "dest": "/api/validate-key.js" }
  ]
}
